{
    "name": "Design of Experiments",
    "description": "To design a task that studies the relationship between input and output variables",
    "templates": [
        {
            "name": "Overview",
            "isOverview": true,
            "description": "Overview board for design of experiments templates",
            "folderName": "{{timestamp}}-{{user.initials}}-doe_overview",
            "command": "--no-input --directory use_case_templates/design_of_experiments --output-dir {{output-dir}} timestamp={{timestamp}} author_name={{user.name}} author_initials={{user.initials}} use_case_name=doe_overview",
            "files": [
                {
                    "type": "board",
                    "path": "doe_overview.board",
                    "active": true
                }
            ]
        },
        {
            "name": "Bayesian Optimization",
            "isOverview": false,
            "description": "To optimize the parameters using a bayesian model",
            "folderName": "{{timestamp}}-{{user.initials}}-bayesian_optimization",
            "command": "--no-input --directory use_case_templates/design_of_experiments --output-dir {{output-dir}} timestamp={{timestamp}} author_name={{user.name}} author_initials={{user.initials}} use_case_name=bayesian_optimization",
            "files": [
                {
                    "type": "board",
                    "path": "bayesian_optimization.board",
                    "active": true
                },
                {
                    "type": "notebook",
                    "path": "bayesian_optimization.ipynb"
                    "active": true
                }
            ]
        },
        {
            "name": "Bayesian Modelling",
            "isOverview": false,
            "description": "To estimate the parameters by modelling parametrized equations",
            "folderName": "{{timestamp}}-{{user.initials}}-bayesian_modelling",
            "command": "--no-input --directory use_case_templates/design_of_experiments --output-dir {{output-dir}} timestamp={{timestamp}} author_name={{user.name}} author_initials={{user.initials}} use_case_name=bayesian_modelling",
            "files": [
                {
                    "type": "board",
                    "path": "bayesian_modelling.board",
                    "active": true
                },
                {
                    "type": "notebook",
                    "path": "bayesian_modelling.ipynb"
                }
            ]
        },
        {
            "name": "Classical Doe",
            "isOverview": false,
            "description": "To estimate the parameters if all trials have to be run in parallel",
            "folderName": "{{timestamp}}-{{user.initials}}-classical_doe",
            "command": "--no-input --directory use_case_templates/design_of_experiments --output-dir {{output-dir}} timestamp={{timestamp}} author_name={{user.name}} author_initials={{user.initials}} use_case_name=classical_doe",
            "files": [
                {
                    "type": "board",
                    "path": "classical_doe.board",
                    "active": true
                },
                {
                    "type": "notebook",
                    "path": "classical_doe.ipynb"
                    "active": true
                }
            ]
        }
    ]
}