    {
      "id": "96d96a01-3876-4c84-bbe2-d48445da52c3",
      "title": "Causal Structure",
      "type_specific": {
        "message": "{\"ops\":[{\"insert\":\"Causal Structure\"},{\"attributes\":{\"header\":1},\"insert\":\"\\n\"},{\"insert\":\"\\nTreatment Parameter:\\nOutcome:\\nCommon causes (confounder for both treatment and outcome):\\nEffect modifiers (confounder only for outcome):\\n\\nDisgraph\"},{\"attributes\":{\"header\":2},\"insert\":\"\\n\"},{\"insert\":\"eg.\\nfixed acidity -> pH; fixed acidity -> quality;\\nvolatile acidity -> pH; volatile acidity -> quality;\\nresidual sugar -> quality; pH -> quality;\\n\\n_________________________________\\nIn the notebook \\\"causal_estimate.ipynb\\\", adjust the respective lines (linked to the card) to\\nspecify the causal structure\"},{\"attributes\":{\"list\":\"bullet\"},\"insert\":\"\\n\"},{\"insert\":\"specify the specific treatment and outcome\"},{\"attributes\":{\"list\":\"bullet\"},\"insert\":\"\\n\"},{\"insert\":\"\\n\"}]}"
      },
      "edge_connections": [
        {
          "id": "12736d6d-e518-4561-b153-b06a06923d31",
          "connector": "top"
        }
      ],
      "type": "note",
      "position": {
        "x": 1468.8890333333338,
        "y": 650.7827683333336
      },
      "size": {
        "width": 360.6148266666668,
        "height": 411.2586666666668
      },
      "color": "#125059"
    },
    {
      "id": "b2521bac-efa0-40b1-9e4a-ca25b6198c0f",
      "title": "Identify, estimate, refute",
      "type_specific": {
        "message": "{\"ops\":[{\"insert\":\"Estimand type\"},{\"attributes\":{\"header\":1},\"insert\":\"\\n\"},{\"insert\":\"nonparametric-ate\\n\\nBackdoor\"},{\"attributes\":{\"header\":1},\"insert\":\"\\n\"},{\"insert\":\"Expression: \\n  d                                             \\n─────(E[quality|volatile acidity,fixed acidity])\\nd[pH]   \\n\\nAssumptions:\\nUnconfoundedness: If U→{pH} and U→quality then P(quality|pH,volatile acidity,fixed acidity,U) = P(quality|pH,volatile acidity,fixed acidity)\"},{\"attributes\":{\"list\":\"ordered\"},\"insert\":\"\\n\"},{\"insert\":\"\\nCausal Estimates:\\nlinear: 0.49575748394223895\"},{\"attributes\":{\"list\":\"bullet\"},\"insert\":\"\\n\"},{\"insert\":\"dml: 0.20754202938065247\"},{\"attributes\":{\"list\":\"bullet\"},\"insert\":\"\\n\"},{\"insert\":\"\\nRefutation Tests:\\nAll passed\\n\\nIV\"},{\"attributes\":{\"header\":1},\"insert\":\"\\n\"},{\"insert\":\"No such variables found\\n\\nFrontdoor\"},{\"attributes\":{\"header\":1},\"insert\":\"\\n\"},{\"insert\":\"No such variables found\\n\\n\\n\\n\"}]}"
      },
      "edge_connections": [
        {
          "id": "efd6bc9f-e08b-4b53-8942-38b7f377356c",
          "connector": "top"
        }
      ],
