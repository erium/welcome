{
    "name": "Prediction",
    "description": "To predict an output based on input parameters provided",
    "templates": [
        {
            "name": "Overview",
            "isOverview": true,
            "description": "Overview board for prediction templates",
            "folderName": "{{timestamp}}-{{user.initials}}-prediction_overview",
            "command": "--no-input --directory use_case_templates/prediction --output-dir {{output-dir}} timestamp={{timestamp}} author_name={{user.name}} author_initials={{user.initials}} use_case_name=prediction_overview",
            "files": [
                {
                    "type": "board",
                    "path": "prediction_overview.board",
                    "active": true
                }
            ]
        },
        {
            "name": "Regression",
            "isOverview": false,
            "description": "To estimate the relationships between a dependent variable and one or more independent variables",
            "folderName": "{{timestamp}}-{{user.initials}}-regression",
            "command": "--no-input --directory use_case_templates/prediction --output-dir {{output-dir}} timestamp={{timestamp}} author_name={{user.name}} author_initials={{user.initials}} use_case_name=regression",
            "files": [
                {
                    "type": "board",
                    "path": "regression.board",
                    "active": true
                },
                {
                    "type": "notebook",
                    "path": "regression.ipynb"
                    "active": true
                }
            ]
        }
    ]
}